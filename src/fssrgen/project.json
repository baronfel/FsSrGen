{
    "version": "3.0.0",
    "buildOptions": {
        "emitEntryPoint": true,
        "compilerName": "fsc",
        "additionalArguments": [ "--standalone" ],
        "compile": {
            "includeFiles": [
                "Program.fs"
            ]
        }
    },

    "dependencies": {
        "FSharp.Core": "4.0.0.1"
    },

    "frameworks": {
        "net46" : { 
            "frameworkAssemblies": {
                "System.Xml": ""
            }
        }
    },

    "tools": {
        "dotnet-compile-fsc": {
            "version": "1.0.0-preview2-*",
            "imports": "dnxcore50"
        }
    }
}

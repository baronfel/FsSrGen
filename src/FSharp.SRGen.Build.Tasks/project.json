            }
        },
        "netstandard1.6":{
            "dependencies": {
                "Microsoft.FSharp.Core.netcore": "1.0.0-alpha-*",
                "NETStandard.Library": "1.6.0",
                "Microsoft.Build": "15.1.262-preview5",
                "Microsoft.Build.Utilities.Core": "15.1.262-preview5",
                "Microsoft.Build.Framework": "15.1.262-preview5"
            }
        } 
    }

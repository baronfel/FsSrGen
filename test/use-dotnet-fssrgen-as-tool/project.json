{
    "version": "1.0.0",

    "buildOptions": {
        "compilerName": "fsc",
        "compile": {
            "includeFiles": [
                "FSComp.fs",
                "Program.fs"
            ]
        },
        "embed": {
            "includeFiles": [
                "FSComp.resx"
            ]
        }
    },

    "tools": {
        "dotnet-fssrgen": { "version": "3.1.*" },
        "dotnet-compile-fsc": {
            "version": "1.0.0-preview1-*",
            "imports": [
                "dnxcore50",
                "portable-net45+win81",
                "netstandard1.3"
            ]
        }
    },

    "testRunner": "xunit",

    "dependencies": {
        "xunit": "2.1.0",
        "dotnet-test-xunit": "1.0.0-rc2-build10025",
        "Microsoft.FSharp.Core.netcore": "1.0.0-alpha-160509",
        "Microsoft.NETCore.App": {
            "type": "platform",
            "version": "1.0.0-rc2-3002702"
        }
    },

    "frameworks": {
        "netcoreapp1.0": { 
            "imports": [
                "dnxcore50",
                "dotnet5.4",
                "netstandardapp1.5",
                "portable-net451+win8"
             ]
        }
    }
}

{
    "version": "1.0.0",
    
    "buildOptions": {
        "emitEntryPoint": true,
        "compilerName": "fsc",
        "compile": {
            "includeFiles": [
                "FSComp.fs",
                "Program.fs"
            ]
        },
        "embed": {
            "includeFiles": [
                "FSComp.resx"
            ]
        }
    },

    "dependencies": {
        "NUnit": "3.2.0",
        "NUnitLite": "3.2.0"
    },

    "frameworks": {
        "net46": {
            "dependencies": {
                "FSharp.Core": "4.0.0.1"
            }
        },
        "netstandard1.5": { 
            "dependencies": {
                "Microsoft.FSharp.Core.netcore": "1.0.0-alpha-160509",
                "Microsoft.NETCore.App": {
                    "type": "platform",
                    "version": "1.0.0-rc2-3002702"
                }
            },
            "imports": [
                "dnxcore50",
                "portable-net451+win8"
             ]
        }
    },

    "tools": {
        "dotnet-compile-fsc": {
            "version": "1.0.0-preview1-*",
            "imports": [
                "dnxcore50",
                "portable-net45+win81",
                "netstandard1.3"
            ]
        }
    }
    
}
